{
    "endOfLine": "auto",
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none",
    "tabWidth": 4,
    "printWidth": 140,
    "bracketSpacing": true,
    "bracketSameLine": true,
    "plugins": ["prettier-plugin-tailwindcss", "prettier-plugin-astro"],
    "overrides": [
        {
            "files": "*.astro",
            "options": {
                "parser": "astro"
            }
        }
    ]
}
